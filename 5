#include <iostream>
#include <string>
using namespace std;
int main() {
	int n,i,j,k;
	string c[] = {"зелёной","красной","жёлтой","белой","чёрной"};
	string a[] = {"крысы","коровы","тигра","зайца","дракона","змеи","лошади","овцы","обезьяны","курицы","собаки","свиньи"};
	cin >> n;
	if (n < 1984)
		cout << "Данные введены некорректно";
	else {
		cout << "Год ";
		i = (n - 1984) % 60;
		k = i / 12;
		j = i % 12;
		if ((j==2) or (j==3) or (j==4)) {
		    c[0]="зелёного";
		    c[1]="красного";
		    c[2]="жёлтого";
		    c[3]="белого";
		    c[4]="чёрного";
		}
		cout << c[k] << " " << a[j];
	}
	return 0;
}
